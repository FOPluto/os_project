<?xml version="1.0"?>
<!--2022ysu-eagle 多线程部分参数调整-->
<opencv_storage>
<num_produce>5</num_produce><!--生产者数量-->
<num_consume>1</num_consume><!--消费者数量-->
<num_sub_consume>2</num_sub_consume><!--次级消费者数量-->
<size_buffer>2</size_buffer><!--生产者缓冲区大小-->
<size_sub_buffer>2</size_sub_buffer><!--消费者缓冲区大小-->
<speed_produce></speed_produce><!--生产者速度-->
<speed_consume>2</speed_consume><!--消费者速度-->
<speed_sub_consume>2</speed_sub_consume><!--次级消费者速度-->
<path>./save.keli</path><!--默认读取文件。初始化上次的-->

</opencv_storage>